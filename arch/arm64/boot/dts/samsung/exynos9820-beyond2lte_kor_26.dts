		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
		samsung,pin-pud-pdn = <0>;
	};
};

&hsi2c_11 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&hsi2c11_bus &fm_int>;

	rtcfmradio@64 {
