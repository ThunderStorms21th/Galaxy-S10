		reg = <0x64>;
		interrupts = <1 0 0>;
		interrupt-parent = <&gpa2>;
		fmint-gpio = <&gpa2 1 0>;
		volume_db;
		radio_vol = <44 43 39 36 33 30 27 25 23 20 17 14 11 8 6 4>;
	};
};

